<Page x:Class="SemiConductor_Equipment.Views.Pages.LoadPort2_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SemiConductor_Equipment.Views.Pages"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoadPort2_Page">

    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <ui:Button x:Name="btnBack" Click="btnBack_Click" Background="Transparent" MouseOverBackground="LightGray" Height="30" Width="40" Margin="5,0,0,0">
                <ui:Button.Icon>
                    <ui:SymbolIcon Symbol="Backspace24" Foreground="Black"/>
                </ui:Button.Icon>
            </ui:Button>
        </Grid>
    </Grid>
</Page>
